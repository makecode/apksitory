div.apps-container.is-list
  ul.apps-container__list
    li.apps-container__el
      div.app
        div.label-wrap.is-red
          div.label
            span 1
        div.app__left
          a(href='#')
            img.lazy(
              src=require('../../media/images/loading.gif')
              data-src=require('../../media/images/ico-small-70.png')
              data-srcset=`${require('../../media/images/ico-small-70.png')} 1x, ${require('../../media/images/ico-big-140.png')} 2x`
            )
        div.app__right
          a(href='#').app__name Minecraft Earth
          span.app__version 2019.1030.04.0
          span.app__view Tools
          div.down
            a(href='#') Download APK
